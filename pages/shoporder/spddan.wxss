.top-tab {
    width: 750rpx;
    height: 100rpx;
    background: white;
    color: #545454;
    font-size: 28rpx;
    line-height: 100rpx;
    box-shadow: 0 2px 2px #bebebe;
    position: fixed;
    z-index: 999;
    top: 0;
    left: 0;
    right: 0;
}

.flex-tab {
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
}

.flex-item {
    flex-grow: 1;
    text-align: center;
}

.toptab {
    border-bottom: solid 3px transparent;
    transition: all 0.4s ease-in-out;
}

.toptab.active {
    font-weight: bold;
    color: #ed7321;
    border-bottom: solid 3px #ed7321;
}

.tab_con {
    height: 100%;
    background: #f5f5f5;
    position: relative;
    margin-top: 100rpx;
    width: 500%;
    left: 0;
    transition: all 0.4s ease-in-out;
}


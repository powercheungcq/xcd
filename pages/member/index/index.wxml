<view class="top" wx:if="{{state==0}}">
    <view class="Head">
        <image class="a" src="{{userInfoAvatar}}" wx:if="{{flag}}"></image>
        <image bindtap="isclicklogin" class="a" src="../../../images/isheadr.png" wx:else></image>
    </view>
    <view class="real">
        <text class="titleText"> {{nickName}}</text>
    </view>
    <view class="myzhanghu ">
        <text>账户金额</text>
        <view>{{mymoney}} 元</view>
    </view>
</view>
<view class="order-area">
    <view bindtap="navigateToOrder2" class="order" data-status="0">
        <image src="../../../images/myhb.png"></image>
        <text>我的红包</text>
    </view>
    <view bindtap="navigateToOrder3" class="order" data-status="1">
        <image src="../../../images/myhyk.png"></image>
        <text>充值卡</text>
    </view>
    <view bindtap="navigateToOrder1" class="order" data-status="2">
        <image src="../../../images/myyhq.png"></image>
        <text>我的优惠券</text>
    </view>
    <view bindtap="navigateToOrder" class="order" data-status="3">
        <image src="../../../images/mysy.png"></image>
        <text>我的收益</text>
    </view>
</view>
<view bindtap="navigateToAddress" class="tableviewcell">
    <image src="../../../images/myinfo.png"></image>
    <text>我的资料</text>
</view>
<view class="tableviewcell">
    <button openType="contact"></button>
    <image src="../../../images/iszhibo.png"></image>
    <text>我要直播</text>
</view>
<view bindtap="navigateToShare" class="tableviewcell">
    <image src="../../../images/mycar.png"></image>
    <text>我的车辆</text>
</view>
<view bindtap="fxuser" class="tableviewcell">
    <image src="../../../images/fx.png"></image>
    <text>我的分享</text>
</view>
<view bindtap="navigateToDonate" class="tableviewcell">
    <image src="../../../images/icon_donate.png"></image>
    <text>商城订单</text>
</view>
<view bindtap="ewm" class="tableviewcell">
    <image src="../../../images/ewm.png"></image>
    <text>我的二维码</text>
</view>
<view bindtap="fankui" class="tableviewcell">
    <image src="../../../images/yhfk.png"></image>
    <text>问题反馈</text>
</view>
<view bindtap="navigateToAboutus" class="tableviewcell">
    <image src="../../../images/sz.png"></image>
    <text>系统设置</text>
</view>

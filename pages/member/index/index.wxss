page {
    background: #eee;
}

.top {
    position: relative;
    display: flex;
    align-items: center;
    height: 250rpx;
    background: rgb(212,235,229);
}

.Head {
    float: left;
    width: 20%;
    height: 140rpx;
    line-height: 140rpx;
    position: relative;
    margin-left: 50rpx;
    background: rgb(212,235,229);
}

.myzhanghu {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 100rpx;
    text-align: center;
    font-size: 28rpx;
    background: rgb(212,235,229);
}

.myzhanghu>view {
    font-size: 33rpx;
    background: rgb(212,235,229);
}

.a {
    float: right;
    width: 100rpx;
    height: 100rpx;
    margin-top: 25rpx;
    margin-right: 30rpx;
    border-radius: 50%;
    background: rgb(212,235,229);
}

.real {
    background: rgb(212,235,229);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 200rpx;
}

.loginbtn {
    margin-left: 50rpx;
}

.loginbtn text {
    padding: 10rpx;
}

.userinfo-nickname {
    color: #333;
}

.tableviewcell {
    padding: 20rpx 40rpx;
    border-bottom: 1rpx solid #ddd;
    color: #666;
    background: white;
    display: flex;
    align-items: center;
    position: relative;
}

.tableviewcell>button {
    width: 100%;
    height: 100%;
    opacity: 0;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 7777;
}

.tableviewcell image {
    width: 45rpx;
    height: 45rpx;
}

.tableviewcell text {
    font-size: 16px;
    margin-left: 10px;
}

.logout {
    margin: 30rpx;
}

.order-area {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background: white;
    margin: 20rpx 0;
    padding: 20rpx 0;
}

.order {
    width: 50%;
    text-align: center;
}

.order image {
    width: 90rpx;
    height: 90rpx;
}

.order text {
    display: block;
    font-size: 30rpx;
    color: #333;
}
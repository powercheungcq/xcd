.myPackage {
    padding: 0 10rpx;
    background: #fff;
}

page {
    background: #fff;
}

.packageList {
    position: relative;
    margin-top: 20rpx;
    display: flex;
    align-items: center;
    background: white;
}

.top,.bottom {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    background: #f2f2f2;
}

.wai {
    width: 100%;
    padding-top: 20rpx;
    background: #f2f2f2;
}

.top {
    top: -20rpx;
    left: 190rpx;
}

.bottom {
    top: 222rpx;
    left: 190rpx;
}

.left {
    margin-top: 20rpx;
    width: 32%;
    text-align: center;
    padding: 40rpx 10rpx;
    border-right: 4rpx #f2f2f2 dashed;
}

.price {
    color: red;
    font-size: 50rpx;
}

.right {
    float: left;
    width: 68%;
    line-height: 50rpx;
    padding: 20rpx 20rpx 20rpx 40rpx;
}

.time {
    font-size: 28rpx;
}

.phone {
    font-size: 28rpx;
    color: #999;
}

.title {
    width: 100%;
    text-align: center;
    margin-top: 20rpx;
}

page {
    background: #fff;
}